Handler(e.target.value, \"email\")}\n                  />\n                  {Err.email && \n                   <InputGroupAddon addonType=\"prepend\">\n                   <InputGroupText>\n                   <i class=\"fas fa-exclamation-circle\" />\n                   </InputGroupText>\n                 </InputGroupAddon>\n                  \n                  }\n                </InputGroup>\n              </FormGroup>\n              <FormGroup>\n                <InputGroup className=\"input-group-alternative\">\n                  <InputGroupAddon addonType=\"prepend\">\n                    <InputGroupText>\n                      <i className=\"ni ni-lock-circle-open\" />\n                    </InputGroupText>\n                  </InputGroupAddon>\n                  <Input\n                    placeholder=\"Password\"\n                    type=\"password\"\n                    autoComplete=\"new-password\"\n                    onChange={(e) => changeHandler(e.target.value, \"password\")}\n                  />\n                  {Err.password && \n                   <InputGroupAddon addonType=\"prepend\">\n                   <InputGroupText>\n                   <i class=\"fas fa-exclamation-circle\" />\n                   </InputGroupText>\n                 </InputGroupAddon>\n                  \n                  }\n                </InputGroup>\n              </FormGroup>\n              <div className=\"text-muted font-italic\">\n                <small>\n                  password strength:{\" \"}\n                  {Err.password ?\n                  <span className={\"text-danger font-weight-700\"}>{Err.password}</span>\n                  :\n                  <span className={`${pswdStrength==='enough'?'text-warning':pswdStrength==='medium'?'text-success':'text-info'} font-weight-700`}>{pswdStrength}</span>\n                  }\n                  \n                </small>\n              </div>\n              <Row className=\"my-4\">\n                <Col xs=\"